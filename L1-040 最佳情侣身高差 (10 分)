import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sca = new Scanner(System.in);
		int n = sca.nextInt();
		for (int i = 0; i < n; i++) {
			String s = sca.next();
			float g = sca.nextFloat();
			if (s.equals("M"))
				System.out.println(String.format("%.2f", g / 1.09));
			else
				System.out.println(String.format("%.2f", g * 1.09));
		}

	}
}
