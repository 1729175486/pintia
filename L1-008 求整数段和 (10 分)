import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sca = new Scanner(System.in);
		int a = sca.nextInt();
		int b = sca.nextInt();
		int c = 0, x = 0;
		for (; a <= b; a++) {
			x += a;
			System.out.printf("%5d", a);
			c++;
			if (c % 5 == 0)
				System.out.println();
		}
		if (c % 5 != 0)
			System.out.println();
		System.out.print("sum = " + x);

	}
}
