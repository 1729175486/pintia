import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sca = new Scanner(System.in);
		int a = sca.nextInt();
		int b = sca.nextInt();
		if (b == 0)
			System.out.printf("%d/0=Error", a);
		else if (b < 0)
			System.out.printf("%d/(%d)=%.2f", a, b, (a * 1.0 / b));
		else
			System.out.printf("%d/%d=%.2f", a, b, (a * 1.0 / b));

	}
}
